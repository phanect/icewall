// Schema for IcedGate
// DO NOT EDIT MANUALLY
// This file should be gitignored

model User {
  id          String    @id
  username    String    @unique
  githubId    Int?      @unique
  authSession Session[]
}

model Session {
  id        String   @id
  userId    String
  expiresAt DateTime
  fresh     Boolean
  user      User     @relation(references: [ id ], fields: [ userId ], onDelete: Cascade)

  @@index([ userId ])
}
